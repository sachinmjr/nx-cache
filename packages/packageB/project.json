{
  "name": "@./packageB",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/packageB/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/packageB",
        "tsConfig": "packages/packageB/tsconfig.lib.json",
        "packageJson": "packages/packageB/package.json",
        "main": "packages/packageB/src/index.ts",
        "assets": ["packages/packageB/*.md"]
      },
      "dependsOn": ["@./packageA:build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/packageB/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/packageB/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "coverageReporters": ["text"]
        }
      }
    }
  },
  "tags": []
}